{
	"name": "svg-preview",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"clean": "rimraf dist",
		"deploy": "gh-pages -t -n -d dist",
		"dist": "yarn clean && yarn build && yarn deploy",
		"dev": "parcel src/index.html",
		"build:worker:editor": "parcel build node_modules/monaco-editor/esm/vs/editor/editor.worker.js --no-source-maps",
		"build:worker": "concurrently yarn:build:worker:*",
		"build:main": "parcel build src/index.html --public-url ./ && nodetouch dist/.nojekyll",
		"build": "concurrently yarn:build:main yarn:build:worker"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^5.11.2",
		"concurrently": "^5.0.0",
		"gh-pages": "^2.1.1",
		"monaco-editor": "^0.18.1",
		"normalize-scss": "^7.0.1",
		"parcel-bundler": "^1.12.4",
		"rimraf": "^3.0.0",
		"rxjs": "^6.5.3",
		"sass": "^1.23.7",
		"touch": "^3.1.0",
		"typescript": "^3.7.3"
	}
}
